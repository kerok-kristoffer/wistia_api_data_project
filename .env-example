# copy to .env and fill in
WISTIA_API_TOKEN=
WISTIA_BASE_URL=https://api.wistia.com/v1

# One or many (comma-separated)
MEDIA_IDS=

# Optional defaults
WISTIA_PAGE=1
WISTIA_PER_PAGE=5


# Infra (non-secret)
AWS_REGION=us-east-1
ENV=dev
PROJECT_SLUG=wistia-pipeline

# If you prefer explicit names (else script will derive)
RAW_BUCKET=
SILVER_BUCKET=
GOLD_BUCKET=
GLUE_TEMP_BUCKET=
ATHENA_RESULTS_BUCKET=

# Optional: single S3 prefix inside each bucket
S3_PREFIX=wistia/

# Optional: KMS (leave empty to use SSE-S3)
KMS_KEY_ARN=